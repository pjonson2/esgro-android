        android:id="@+id/responedToTxt"
        android:layout_width="55dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:text="Ben"
        android:textColor="#393E46"
        android:textSize="12sp"
        app:fontFamily="@font/open_sans"
        app:layout_constraintEnd_toEndOf="@+id/view51"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="@+id/view51"
        app:layout_constraintTop_toTopOf="@+id/view51" />

    <TextView
        android:id="@+id/responedDescriptionTxt"
        android:layout_width="304dp"
        android:layout_height="133dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:editable="true"
        android:enabled="true"
        android:text="Hello Ben, I used single color LED lights because color changing LED lights are more expensive than the price I estimated.|"
        android:textColor="#929AAB"
        android:textSize="12sp"
        android:textStyle="bold"
        app:fontFamily="@font/open_sans"
        app:layout_constraintBottom_toTopOf="@+id/textView88"
        app:layout_constraintEnd_toEndOf="@+id/view51"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view51"
        app:layout_constraintTop_toTopOf="@+id/view51"
        app:layout_constraintVertical_bias="0.666"
        tools:ignore="Deprecated" />

    <Button
        android:id="@+id/button30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
